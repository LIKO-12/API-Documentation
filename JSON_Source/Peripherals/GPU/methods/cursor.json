{
	"desc":"Sets the current active mouse cursor, or creates a new one.",
	"usage":[
		{
			"name":"Set the current mouse cursor:",
			"args":[
				{
					"name":"name",
					"type":"string",
					"desc":"The name of the cursor."
				},
				{
					"name":"grap",
					"type":"boolean",
					"desc":"True -> Grap the cursor to the pixelated screen.",
					"default":"false"
				}
			]
		},
		{
			"name":"Get the current mouse cursor:",
			"rets":[
				{
					"name":"current",
					"type":"string",
					"desc":"The current active cursor name."
				}
			]
		},
		{
			"name":"Create a new cursor:",
			"args":[
				{
					"name":"imgdata",
					"type":"ImageData",
					"desc":"The imagedata of the cursor."
				},
				{
					"name":"name",
					"type":"string",
					"desc":"The name of the cursor.",
					"default":"default"
				},
				{
					"name":"hx",
					"type":"number",
					"desc":"The X coord of the cursor hot position.",
					"default":"0"
				},
				{
					"name":"hy",
					"type":"number",
					"desc":"The Y coord of the cursor hot position.",
					"default":"0"
				}
			]
		}
	]
}